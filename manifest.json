{
  "name": "Navigator Prototype",
  "description": "Gives you the best idea about your current browsing location you could possibly want.",
  "version": "1.0",
  "permissions": [
    "contextMenus",
    "tabs",
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.wikipedia.org/*",
        "http://localhost/*"
      ],
      "css": [
        "lib/css/quote_graph.css",
        "lib/css/materialize.css",
        "lib/css/jquery-editable.css"
      ],
      "js": [
        "lib/d3.js",
        "lib/jquery-2.2.0.js",
        "lib/jChangeType.js",
        "lib/sigma.js/sigma.min.js",
        "lib/sigma.js/plugins/sigma.parsers.json/sigma.parsers.json.js",
        "lib/sigma.js/plugins/sigma.renderers.customShapes/shape-library.js",
        "lib/sigma.js/plugins/sigma.renderers.customShapes/sigma.renderers.customShapes.js",
        "lib/jsPlumb-2.0.5.js",
        "lib/jquery.panzoom.js",
        "scripts/shared/constants.js",
        "scripts/shared/utils.js",
        "scripts/shared/quote_storage.js",
        "scripts/shared/history_storage.js",
        "scripts/content/history_graph.js",
        "scripts/content/quote_graph.js",
        "scripts/content/content_script.js",
        "lib/materialize.js",
        "lib/jquery-editable-poshytip.js"
      ],
      "run_at": "document_start"
    }
  ],
  "background": {
    "scripts": [
      "lib/jquery-2.2.0.js",
      "scripts/shared/constants.js",
      "scripts/shared/utils.js",
      "scripts/shared/quote_storage.js",
      "scripts/shared/history_storage.js",
      "scripts/background/background.js"
    ]
  },
  "web_accessible_resources": [
    "assets/*"
  ],
  "manifest_version": 2
}